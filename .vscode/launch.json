{
  "compounds": [
    {
      "name": "[All]",
      "configurations": ["Server", "Client", "Client"],
      "stopAll": true
    },
    {
      "name": "Client (2)",
      "configurations": ["Client", "Client"],
      "stopAll": true
    }
  ],
  "configurations": [
    {
      "name": "Client",
      "type": "godot",
      "request": "launch",
      "project": "",
      "debug_collisions": false,
      "debug_paths": false,
      "debug_navigation": false,
      "additional_options": "--path ${workspaceFolder}/game"
    },
    {
      "name": "Go",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/game/addons/signal_ws/server/cmd",
      "cwd": "${workspaceFolder}/game/addons/signal_ws/server"
    },
    {
      "name": "Server",
      "type": "godot",
      "request": "launch",
      "project": "",
      "debug_collisions": false,
      "debug_paths": false,
      "debug_navigation": false,
      "additional_options": "--path ${workspaceFolder}/signal-ws-server"
    }
  ]
}
